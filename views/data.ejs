          <% } else {%> <li><a href="/login">Login</a></li> <%}%> 
        </ul>
      </nav><!-- .navbar -->
    </div>
  </header><!-- End Header -->
  <div class="space"></div>
  <div>
    <!--filter drop down by interest-->
    <form action="/datafiltered" method="get" id="filterform" name="filterform" style="margin: 20px;">
      <label for="interest" style="color: white;"><b>Interest:</b></label>
      <select id="interest" name="interest">
          <option>Select an interest</option>
          <% for (let iCount = 0; iCount < leads.length; iCount++) { %>
              <option value="<%= leads[iCount].interest %>">
              <%= leads[iCount].interest %>
              </option>
          <% } %>
      </select>
      <input type="submit" class="logout-link submit-button" value="Submit">
      <input type="button" class="logout-link clear-button" value="Clear Selection" onclick="clearSelection()">
    </form>
  </div>
  <div>
    <table>
      <thead>
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Phone</th>
          <th>Email</th>
          <th>Interest</th>
        </tr>
      </thead>
      <tbody>
        <% for (let iCount = 0; iCount < leads.length; iCount++) { %>
          <tr>
            <td><%= leads[iCount].first_name %></td>
            <td><%= leads[iCount].last_name %></td>
            <td><%= leads[iCount].phone %></td>
            <td><%= leads[iCount].email %></td>
            <td><%= leads[iCount].interest %></td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </div>
